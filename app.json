{
  "expo": {
    "name": "fleet",
    "slug": "fleet",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#202024"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#202024"
      },
      "package": "com.lucasdesouzaesousa_fleet"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.lucasdesouzaesousa_fleet"
        }
      ],
      "expo-font"
    ],
    "extra": {
      "eas": {
        "projectId": "804b4f75-02f9-497f-b7c6-cbd2516d3e14"
      }
    }
  }
}
